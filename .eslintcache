>\r\n          <DragAndDropPlay/>\r\n        </Route>\r\n        <Route exact path=\"/drag-and-drop-play2\">\r\n          <DragAndDrop2 />\r\n        </Route>\r\n        <Route exact path=\"/routines-dnd\">\r\n          <RoutinesDND weekData={weekData} setWeekData={setWeekData}/>\r\n        </Route>\r\n        \r\n      </Switch>\r\n      <div style={{padding: '20px'}}>\r\n        \r\n        <pre>{JSON.stringify(weekData, '' ,4)}</pre>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\gsc22\\OneDrive\\Documents\\Lambda\\Tutorials\\MomentLibrary\\calendar\\src\\main_calendar\\Calendar.js",[],"C:\\Users\\gsc22\\OneDrive\\Documents\\Lambda\\Tutorials\\MomentLibrary\\calendar\\src\\main_calendar\\build.js",[],"C:\\Users\\gsc22\\OneDrive\\Documents\\Lambda\\Tutorials\\MomentLibrary\\calendar\\src\\main_calendar\\styles.js",[],"C:\\Users\\gsc22\\OneDrive\\Documents\\Lambda\\Tutorials\\MomentLibrary\\calendar\\src\\main_calendar\\CalendarHeader.js",[],"C:\\Users\\gsc22\\OneDrive\\Documents\\Lambda\\Tutorials\\MomentLibrary\\calendar\\src\\menu\\Menu.js",["125"],"import React from 'react'\r\nimport {Link, useLocation} from 'react-router-dom'\r\nimport './Menu.css'\r\n\r\nconst Menu = () => {\r\n  \r\n  const pathname = useLocation().pathname\r\n\r\n  function isActivePath(){\r\n    if(pathname === \"/my-routines\" || pathname === \"/create-routine\" || pathname === \"/find-routine\" || pathname === \"/browse-exercises\" ){\r\n      return \"active\"\r\n    }\r\n\r\n    return \"\"\r\n  }\r\n\r\n  return (\r\n    <div className=\"menu\">\r\n      <nav className=\"navbar navbar-expand-lg navbar-dark bg-dark\">\r\n        <div className=\"container-fluid\">\r\n          <Link className=\"navbar-brand\" to=\"/\">YouFit</Link>\r\n          <button className=\"navbar-toggler\" type=\"button\" data-bs-toggle=\"collapse\" data-bs-target=\"#navbarNav\" aria-controls=\"navbarNav\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">\r\n            <span className=\"navbar-toggler-icon\"></span>\r\n          </button>\r\n          <div className=\"collapse navbar-collapse\" id=\"navbarNav\">\r\n            <ul className=\"navbar-nav\">\r\n              <li className=\"nav-item\">\r\n                <Link className=\"nav-link\" aria-current=\"page\" to=\"/schedule\">Schedule</Link>\r\n              </li>\r\n              <li className=\"nav-item\">\r\n                <Link className=\"nav-link\" to=\"/goals\">Goals</Link>\r\n              </li>\r\n              <li className=\"nav-item\">\r\n                <Link className=\"nav-link\" to=\"/progress\">Progress</Link>\r\n              </li>\r\n              <li className=\"nav-item\">\r\n                <Link className=\"nav-link\" to=\"/drag-and-drop-play\">Drag and Drop Play</Link>\r\n              </li>\r\n              <li className=\"nav-item\">\r\n                <Link className=\"nav-link\" to=\"/drag-and-drop-play2\">Drag and Drop Play 2</Link>\r\n              </li><li className=\"nav-item\">\r\n                <Link className=\"nav-link\" to=\"/routines-dnd\">Routines DnD</Link>\r\n              </li>\r\n              <li className={`${isActivePath()} nav-item dropdown`}>\r\n                <a className=\"nav-link dropdown-toggle\" href=\"#\" id=\"navbarDropdown\" role=\"button\" data-bs-toggle=\"dropdown\" aria-expanded=\"false\">\r\n                  Workout Routines\r\n                </a>\r\n                <ul className=\"dropdown-menu\" aria-labelledby=\"navbarDropdown\">\r\n                  <li><Link className=\"dropdown-item\" to=\"/my-routines\">My Saved Routines</Link></li>\r\n                  <li><Link className=\"dropdown-item\" to=\"/create-routine\">Create New Routine</Link></li>\r\n                  <li><Link className=\"dropdown-item\" to=\"/find-routine\">Find Routines</Link></li>\r\n                  <li><hr className=\"dropdown-divider\" /></li>\r\n                  <li><Link className=\"dropdown-item\" to=\"/browse-exercises\">Browse Exercises</Link></li>\r\n                </ul>\r\n              </li>\r\n            </ul>\r\n          </div>\r\n        </div>\r\n      </nav>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default Menu\r\n","C:\\Users\\gsc22\\OneDrive\\Documents\\Lambda\\Tutorials\\MomentLibrary\\calendar\\src\\routines\\CreateRoutine.js",[],"C:\\Users\\gsc22\\OneDrive\\Documents\\Lambda\\Tutorials\\MomentLibrary\\calendar\\src\\routines\\FindRoutine.js",[],"C:\\Users\\gsc22\\OneDrive\\Documents\\Lambda\\Tutorials\\MomentLibrary\\calendar\\src\\routines\\MyRoutines.js",[],"C:\\Users\\gsc22\\OneDrive\\Documents\\Lambda\\Tutorials\\MomentLibrary\\calendar\\src\\routines\\BrowseExercises.js",[],"C:\\Users\\gsc22\\OneDrive\\Documents\\Lambda\\Tutorials\\MomentLibrary\\calendar\\src\\goals\\Goals.js",[],"C:\\Users\\gsc22\\OneDrive\\Documents\\Lambda\\Tutorials\\MomentLibrary\\calendar\\src\\progress\\Progress.js",[],"C:\\Users\\gsc22\\OneDrive\\Documents\\Lambda\\Tutorials\\MomentLibrary\\calendar\\src\\dashboard\\Dashboard.js",[],"C:\\Users\\gsc22\\OneDrive\\Documents\\Lambda\\Tutorials\\MomentLibrary\\calendar\\src\\drag_n_drop_play\\DragAndDropPlay.js",[],"C:\\Users\\gsc22\\OneDrive\\Documents\\Lambda\\Tutorials\\MomentLibrary\\calendar\\src\\drag_n_drop_play\\initial-data.js",[],"C:\\Users\\gsc22\\OneDrive\\Documents\\Lambda\\Tutorials\\MomentLibrary\\calendar\\src\\drag_n_drop_play\\Column.js",["126"],"import React from 'react'\r\nimport styled from 'styled-components'\r\nimport {Droppable, Draggable} from 'react-beautiful-dnd'\r\nimport Task from './Task'\r\n\r\n\r\nconst TaskList = styled.div`\r\n  padding: 8px\r\n`\r\nconst Column = ({column, tasks}) => {\r\n\r\n  return (\r\n    <div className=\"task-list-container container\">\r\n      <h3 className=\"title\" style={{padding: '8px'}}>{column.title}</h3>\r\n      {/* \r\n        Droppable expects it's child to be a function that returns a react component. \r\n        One reason the render props pattern is used, is because rbdnd does not have to create any DOM nodes for you. \r\n        You create you components the way you want to and rbdnd latches into your existing structure. \r\n\r\n        Need to spread in the provided.droppableProps objecct into the style-components element \r\n        A styled-components has a callback function called innerRef and we can use it to pass provided.innerRef\r\n        !!innerRef is a function used to supply a DOM node of your component to rbdnd\r\n\r\n        (note on above paragraph: changed innerRef to just ref following the example in the documentation and it worked)\r\n\r\n        provided.placeholder is a React element used to create more space in a droppable during a drag when it is needed.\r\n        the placeholder needs to be added as a child of the component that we have designated ast the 'droppable' (TaskList)\r\n      */}\r\n      <Droppable\r\n        droppableId={column.id} // Droppables have one required prop - a droppableId \r\n      >\r\n        {(provided) => (\r\n          <div\r\n          className='task-list'\r\n          ref={provided.innerRef}\r\n          {...provided.droppableProps}\r\n          >\r\n            {tasks.map((task, index) => \r\n            \r\n            <Draggable \r\n            key={task.id} \r\n            draggableId={task.id}\r\n            index={index}>\r\n\r\n              {(provided, snapshot) => {\r\n                console.log({tasks})\r\n                return(<Task task={task} provided={provided} snapshot={snapshot}  />)\r\n              }}\r\n\r\n            </Draggable>)}\r\n            {provided.placeholder} \r\n          </div>\r\n        )}\r\n      </Droppable>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default Column\r\n","C:\\Users\\gsc22\\OneDrive\\Documents\\Lambda\\Tutorials\\MomentLibrary\\calendar\\src\\drag_n_drop_play\\Task.js",["127"],"import React from 'react'\r\nimport styled from 'styled-components'\r\n\r\nconst Container = styled.div`\r\n  border: 1px solid lightgrey;\r\n